{
    "fuzzers": [
        {
            "target_args": [
                "-f",
                "root/etc/lighttpd/lighttpd.conf",
                "-D",
                "-m",
                "root/etc/lighttpd/modules/"
            ],
            "target": "root/usr/sbin/lighttpd",
            "library_path": "root/usr/lib/x86_64-linux-gnu:root/lib/x86_64-linux-gnu",
            "network": {
                "url": "tcp://localhost:8080",
                "is_client": false,
                "timeout": 1000
            }
        }
    ]
}
